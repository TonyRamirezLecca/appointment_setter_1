<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.ComboBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.PatientUpdateController">
    <children>
        <TextField fx:id="name" layoutX="226.0" layoutY="84.0" promptText="name" />
        <TextField fx:id="insurance" layoutX="226.0" layoutY="124.0" promptText="insurance provider" />
        <TextField fx:id="address1" layoutX="226.0" layoutY="166.0" promptText="address line 1" />
        <TextField fx:id="address2" layoutX="226.0" layoutY="191.0" promptText="address line 2" />
        <TextField fx:id="city" layoutX="226.0" layoutY="216.0" promptText="city" />
        <ComboBox fx:id="state" layoutX="228.0" layoutY="241.0" prefHeight="25.0" prefWidth="145.0" />
        <TextField fx:id="postal_code" layoutX="226.0" layoutY="266.0" promptText="postal code" />
        <TextField fx:id="phone" layoutX="226.0" layoutY="291.0" promptText="phone number" />
        <Button layoutX="228.0" layoutY="330.0" mnemonicParsing="false" onAction="#updatePatientHandler" prefHeight="25.0" prefWidth="149.0" text="Save" />
        <Label layoutX="230.0" layoutY="23.0" text="Update Patient">
            <font>
                <Font size="27.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
